{
  "name": "marketinsight",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
    "test": "jest",
    "prisma": "node -r ./prisma/load-env.mjs ./node_modules/.bin/prisma",
    "prisma:generate": "node -r ./prisma/load-env.mjs ./node_modules/.bin/prisma generate",
    "prisma:push": "node -r ./prisma/load-env.mjs ./node_modules/.bin/prisma db push",
    "prisma:migrate": "node -r ./prisma/load-env.mjs ./node_modules/.bin/prisma migrate dev",
    "prisma:studio": "node -r ./prisma/load-env.mjs ./node_modules/.bin/prisma studio",
    "db:reset": "node -r ./prisma/load-env.mjs ./node_modules/.bin/prisma migrate reset"
  },
  "dependencies": {
    "@langchain/community": "^0.3.55",
    "@langchain/core": "^0.3.75",
    "@langchain/deepseek": "^0.1.0",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/openai": "^0.6.11",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "jsonwebtoken": "^9.0.2",
    "opossum": "^9.0.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/opossum": "^8.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "jest": "^30.0.5",
    "jest-mock-extended": "^4.0.0",
    "jiti": "^2.5.1",
    "prisma": "^6.14.0",
    "prismock": "^1.35.4",
    "ts-jest": "^29.4.1",
    "typescript-eslint": "^8.44.0"
  }
}
